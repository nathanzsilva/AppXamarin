<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                  xmlns:mst="clr-namespace:App2.TipoPagina.Master"
             x:Class="App2.TipoPagina.Master.Master">
    <MasterDetailPage.Master>
        <!--Menu-->
        <ContentPage Title="Menu">
            <StackLayout>
                <Button Text="Pagina 1" BackgroundColor="Transparent" Clicked="MudarPagina1"/>
                <Button Text="Pagina 2" BackgroundColor="Transparent" Clicked="MudarPagina2"/>
                <Button Text="Conteudo" BackgroundColor="Transparent" Clicked="MudarPagina3"/>
            </StackLayout>
            
        </ContentPage>
    </MasterDetailPage.Master>
    <MasterDetailPage.Detail>
        <!--Conteudo-->
        <mst:Conteudo/>
    </MasterDetailPage.Detail>
</MasterDetailPage>